		"@vue/test-utils": "^2.4",
		"element-plus": "^2.7",
		"eslint": "^8.57",
		"eslint-plugin-import": "^2.29",
		"eslint-plugin-prefer-arrow": "^1.2",
		"eslint-plugin-vue": "^9.24",
		"jsdom": "^24.0",
		"path": "^0.12",
		"rollup": "^4.14",
		"rollup-plugin-typescript2": "^0.36",
		"rollup-plugin-dts": "^6.1",
		"terser": "^5.30",
		"typescript": "^5.4",
		"unplugin-auto-import": "^0.17",
		"unplugin-vue-components": "^0.26",
		"vite": "^5.2",
		"vite-plugin-dts": "^3.8",
		"vitest": "^1.4",
		"vue": "^3.4",
		"vue-tsc": "^2.0",
		"zod": "^3.22",
		"yup": "^1.4",
		"valibot": "^0.30",
		"primevue": "^3.51",
		"@ionic/vue": "^7.8",
		"vue-formify": "^0.0.33"
	},
	"license": "MIT",
	"repository": {
